{
  "name": "mitloshuk/hcaptcha",
  "description": "Library for hCaptcha, a service that protects websites from spam, protects user privacy, rewards websites, and helps companies get their data labeled",
  "type": "library",
  "keywords": [
    "hCaptcha",
    "antispam",
    "anti-spam",
    "captcha",
    "spam",
    "recaptcha"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Artem Mitloshuk",
      "email": "artem@mitloshuk.name"
    }
  ],
  "version": "1.1.0",
  "require": {
    "php": ">=5.6",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "hCaptcha\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "hCaptcha\\Tests\\": "tests/"
    }
  }
}
